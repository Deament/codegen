	<entry key="save">true</entry>
	<entry key="batchadd">true</entry>
	<entry key="savewithid">true</entry>
	<entry key="query1">true</entry>
	<entry key="query">true</entry>
	<entry key="getwithid">true</entry>
	<entry key="remove">true</entry>
	<entry key="batchremove">true</entry>
	<entry key="update">true</entry>
	<entry key="batchupdate">true</entry>
	<entry key="isselect">false</entry>
	<entry key="defaultview">false</entry>
	<entry key="defaultpre">false</entry>
	<entry key="havajs">true</entry>
	<entry key="havahtml">true</entry>
	<entry key="key">id</entry>
	<entry key="valuename">name</entry>
	<entry key="basepackage">cn.qton.backagename</entry>
